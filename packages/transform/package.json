{
  "name": "@joshdb/transform",
  "version": "1.0.0",
  "description": "A Josh middleware which transforms data before and after the provider",
  "author": "@joshdb",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "sideEffects": false,
  "scripts": {
    "test": "vitest run",
    "docs": "typedoc-json-parser",
    "lint": "eslint src tests --ext ts --fix -c ../../.eslintrc",
    "build": "tsup",
    "prepack": "yarn build",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run"
  },
  "dependencies": {
    "@joshdb/provider": "2.0.0-next.c7aa6bf.0",
    "@sapphire/utilities": "^3.15.3"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^3.0.3",
    "@swc/core": "^1.4.17",
    "@vitest/coverage-v8": "^1.5.2",
    "typedoc": "^0.25.13",
    "typedoc-json-parser": "^9.0.1",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josh-development/middlewares.git"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=20",
    "npm": ">=7"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/josh-development/middlewares/issues"
  },
  "homepage": "https://josh.evie.dev",
  "publishConfig": {
    "access": "public"
  }
}
